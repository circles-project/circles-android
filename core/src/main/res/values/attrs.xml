<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="LoadingButton">
        <attr name="android:text" />
        <attr name="android:textSize" />
        <attr name="textPadding" format="dimension" />
        <attr name="android:enabled" />
    </declare-styleable>

    <declare-styleable name="NotificationCounterView">
        <attr name="notificationCircleBackground" format="reference|color" />
    </declare-styleable>

    <declare-styleable name="SettingsMenuItemView">
        <attr name="optionName" format="string" />
        <attr name="optionIcon" format="reference" />
        <attr name="hasDivider" format="boolean" />
    </declare-styleable>

    <declare-styleable name="SettingsGroupLayout">
        <attr name="groupName" format="string" />
    </declare-styleable>

</resources>